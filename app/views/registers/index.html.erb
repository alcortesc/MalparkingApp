<p id="notice"><%= notice %></p>

<h2>Registrar</h2>

<table>
  <thead>
    <tr>
      <th>Evidencia</th>
      <th>Vehiculo</th>
      <th>Usuario</th>
      <th>Reporte</th>
      <th>Fecha hora</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @registers.each do |register| %>
      <tr>
        <td><%= register.evidence %></td>
        <td><%= register.vehicle %></td>
        <td><%= register.user %></td>
        <td><%= register.report %></td>
        <td><%= register.fecha_hora %></td>
        <td><%= link_to 'Show', register %></td>
        <td><%= link_to 'Edit', edit_register_path(register) %></td>
        <td><%= link_to 'Destroy', register, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo Registro', new_register_path %>
