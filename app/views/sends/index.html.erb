<p id="notice"><%= notice %></p>

<h2>Enviar</h2>

<table>
  <thead>
    <tr>
      <th>Notificacion</th>
      <th>Reporte</th>
      <th>Fecha hora</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sends.each do |send| %>
      <tr>
        <td><%= send.notification %></td>
        <td><%= send.report %></td>
        <td><%= send.fecha_hora %></td>
        <td><%= link_to 'Show', send %></td>
        <td><%= link_to 'Edit', edit_send_path(send) %></td>
        <td><%= link_to 'Destroy', send, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo Envio', new_send_path %>
